{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ES2022",
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"moduleResolution": "Node",
		"jsx": "react-jsx",
		"sourceRoot": "./src",
		"rootDir": ".",
		"sourceMap": true,
		"incremental": true,
		"paths": {
			"*": ["."]
		}
	},
	"exclude": [
		"node_modules"
	],
}
